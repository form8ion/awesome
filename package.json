    "@form8ion/remark-lint-preset": "6.0.7",
    "ban-sensitive-files": "1.10.9",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "lockfile-lint": "4.14.1",
    "npm-run-all2": "8.0.4",
    "remark-cli": "12.0.1",
    "remark-toc": "9.0.0"
